{
    "name": "angular-multi-module",
    "version": "1.0.0",
    "description": "A boilerplate for angular multi module configuration",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint ./**/src",
        "e2e": "ng e2e",
        "prettier": "prettier \"./**/src/{app,environments,assets}/**/*.{ts,js,css,scss}\" --write",
        "prettier:check": "prettier --list-different \"./**/src/{app,environments,assets}/**/*.{ts,js,css,scss}\"",
        "precommit": "lint-staged",
        "prepush": "ng lint --quiet  ./**/src"
    },
    "lint-staged": {
        "*.{ts,js,css,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.1",
        "@angular/common": "^9.0.1",
        "@angular/compiler": "^9.0.1",
        "@angular/core": "^9.0.1",
        "@angular/forms": "^9.0.1",
        "@angular/http": "^7.2.16",
        "@angular/platform-browser": "^9.0.1",
        "@angular/platform-browser-dynamic": "^9.0.1",
        "@angular/router": "^9.0.1",
        "@ngrx/db": "^2.1.0",
        "@ngrx/effects": "^8.6.0",
        "@ngrx/router-store": "^8.6.0",
        "@ngrx/store": "^8.6.0",
        "@ngrx/store-devtools": "^8.6.0",
        "@ngx-translate/core": "^12.1.1",
        "core-js": "^3.6.4",
        "ng2-webstorage": "^2.0.0",
        "ngrx-store-freeze": "^0.2.3",
        "ngrx-store-localstorage": "^8.0.0",
        "ngx-cookie": "^4.0.2",
        "rxjs": "^6.0.0",
        "rxjs-compat": "^6.2.0",
        "zone.js": "^0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.2",
        "@angular/cli": "~9.0.2",
        "@angular/compiler-cli": "^9.0.1",
        "@angular/language-service": "^9.0.1",
        "@types/jasmine": "~3.5.5",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~13.7.2",
        "codelyzer": "~5.2.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.2",
        "lint-staged": "^10.0.7",
        "prettier": "1.19.1",
        "protractor": "~5.4.3",
        "ts-node": "~8.6.2",
        "tslint": "~6.0.0",
        "typescript": "~3.7.5"
    }
}